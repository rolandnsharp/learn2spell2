{
  "name": "Learn 2 Spell",
  "description": "Learn spelling and vocabulary with touch-typing drills. Save words you come across on internet using the right-click menu.",
  "version": "4.2",
  "manifest_version": 2,

"background": {
    "page": "background.html"
  },

"browser_action": {
    "default_icon": "img/2.png"

  },


  "icons": {
    "128": "img/icon.png",
    "16": "img/2.png"
  },
  "permissions": [
    "background",
    "tabs",
    "contextMenus",
    "storage",
    "unlimitedStorage",
    "notifications"]

}
